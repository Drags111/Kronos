function SmartGetFieldArrayInt(const Parent: Integer; const Path: AnsiString): TIntArray; overload;
var
  L, I: Integer;
begin
  SetLength(Result, 0);

  if Parent < 0 then
    Exit;

  L := SmartGetFieldArraySize(Parent, Path, 1);
  if(L < 1)then
    Exit;
  SetLength(Result, L);

  if(L <= 0)then
    Exit;

  for I := 0 to L-1 do
    Result[I] := SmartGetFieldArrayInt(Parent, Path, I);
end;

