procedure SmartFreeObject(const Obj: Integer); overload;
begin
  if(Obj < 0)then
    Exit;
  _SmartFreeObject(Obj);
end;

procedure SmartFreeObject(const Objs: TIntArray); overload;
var
  I, H: Integer;
begin
  H := High(Objs);
  if(H < 0)then
    Exit;

  for I := 0 to H do
  begin
    if(Objs[I] < 1) or (SmartIsNull(Objs[I]))then
      Continue;
    _SmartFreeObject(Objs[I]);
  end;
end;

