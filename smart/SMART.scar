function SmartGetFieldString(const Parent: Integer; const Path: AnsiString): String;
var
  Bytes: Integer;
begin
  Result := '';
  if Parent < 0 then
    Exit;
  Bytes := SmartGetFieldObject(Parent, PAnsiChar(Path));
  Result := GetJavaString(Bytes, 512);
  SmartFreeObject(Bytes);
end;

